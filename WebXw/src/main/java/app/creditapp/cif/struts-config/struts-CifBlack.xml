<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="CifBlackAction" namespace="/" extends="struts-cms">
		<!--tab -->
		<action name="CifBlackAction_getTab" method="getTab" class="app.creditapp.cif.action.CifBlackAction">
			<result name="tab">/creditapp/fund/FundBase_tab.jsp</result>
		</action>
	    <!--分页查询 -->
		<action name="CifBlackAction_findByPage" method="findByPage" class="app.creditapp.cif.action.CifBlackAction">
			<result name="list">/creditapp/cif/CifBlack_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="CifBlackAction_input" method="input" class="app.creditapp.cif.action.CifBlackAction">
			<result name="input">/creditapp/cif/CifBlack_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="CifBlackAction_del" method="del" class="app.creditapp.cif.action.CifBlackAction">
			<result name="list">/creditapp/cif/CifBlack_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="CifBlackAction_getById" method="getById" class="app.creditapp.cif.action.CifBlackAction">
			<result name="detail">/creditapp/cif/CifBlack_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="CifBlackActionInsert" method="insert" class="app.creditapp.cif.action.CifBlackAction">
			<result name="input">/creditapp/cif/CifBlack_Insert.jsp</result>
			<result name="detail">/creditapp/cif/CifBlack_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="CifBlackActionUpdate" method="update" class="app.creditapp.cif.action.CifBlackAction">
			<result name="input">/creditapp/cif/CifBlack_Detail.jsp</result>
			<result name="detail">/creditapp/cif/CifBlack_Detail.jsp</result>
		</action>
	</package>
</struts>



