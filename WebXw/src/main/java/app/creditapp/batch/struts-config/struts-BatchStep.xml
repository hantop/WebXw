<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="BatchStepAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="BatchStepAction_findByPage" method="findByPage" class="app.creditapp.batch.action.BatchStepAction">
			<result name="list">/creditapp/batch/BatchStep_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="BatchStepAction_input" method="input" class="app.creditapp.batch.action.BatchStepAction">
			<result name="input">/creditapp/batch/BatchStep_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="BatchStepAction_del" method="del" class="app.creditapp.batch.action.BatchStepAction">
			<result name="list">/creditapp/batch/BatchStep_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="BatchStepAction_getById" method="getById" class="app.creditapp.batch.action.BatchStepAction">
			<result name="detail">/creditapp/batch/BatchStep_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="BatchStepActionInsert" method="insert" class="app.creditapp.batch.action.BatchStepAction">
			<result name="input">/creditapp/batch/BatchStep_Insert.jsp</result>
			<result name="detail">/creditapp/batch/BatchStep_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="BatchStepActionUpdate" method="update" class="app.creditapp.batch.action.BatchStepAction">
			<result name="input">/creditapp/batch/BatchStep_Detail.jsp</result>
			<result name="detail">/creditapp/batch/BatchStep_Detail.jsp</result>
		</action>
	</package>
</struts>



