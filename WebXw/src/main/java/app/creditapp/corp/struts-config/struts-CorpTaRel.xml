<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="CorpTaRelAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="CorpTaRelAction_findByPage" method="findByPage" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="list">/creditapp/corp/CorpTaRel_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="CorpTaRelAction_input" method="input" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="input">/creditapp/corp/CorpTaRel_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="CorpTaRelAction_del" method="del" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="list">/creditapp/corp/CorpTaRel_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="CorpTaRelAction_getById" method="getById" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="detail">/creditapp/corp/CorpTaRel_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="CorpTaRelActionInsert" method="insert" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="input">/creditapp/corp/CorpTaRel_Insert.jsp</result>
			<result name="detail">/creditapp/corp/CorpTaRel_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="CorpTaRelActionUpdate" method="update" class="app.creditapp.corp.action.CorpTaRelAction">
			<result name="input">/creditapp/corp/CorpTaRel_Detail.jsp</result>
			<result name="detail">/creditapp/corp/CorpTaRel_Detail.jsp</result>
		</action>
	</package>
</struts>



