<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="FundProvSplitTermAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="FundProvSplitTermAction_findByPage" method="findByPage" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="list">/creditapp/fund/FundProvSplitTerm_List.jsp</result>
		</action>
	    <!--管理费用分页查询 -->
		<action name="FundProvSplitTermAction_Manager_findByPage" method="Manager_findByPage" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="list">/creditapp/fund/FundProvSplitTermmanager_List.jsp</result>
		</action>		
		<!--获取新增页面-->
		<action name="FundProvSplitTermAction_input" method="input" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="input">/creditapp/fund/FundProvSplitTerm_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="FundProvSplitTermAction_del" method="del" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="list">/creditapp/fund/FundProvSplitTerm_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="FundProvSplitTermAction_getById" method="getById" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="detail">/creditapp/fund/FundProvSplitTerm_Detail.jsp</result>
		</action>

		<!--管理费查询 -->
		<action name="FundProvSplitTermAction_Manager_getById" method="Manager_getById" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="detail">/creditapp/fund/FundProvSplitTermmanager_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="FundProvSplitTermActionInsert" method="insert" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="input">/creditapp/fund/FundProvSplitTerm_Insert.jsp</result>
			<result name="detail">/creditapp/fund/FundProvSplitTerm_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="FundProvSplitTermActionUpdate" method="update" class="app.creditapp.fund.action.FundProvSplitTermAction">
			<result name="input">/creditapp/fund/FundProvSplitTerm_Detail.jsp</result>
			<result name="detail">/creditapp/fund/FundProvSplitTerm_Detail.jsp</result>
		</action>
	</package>
</struts>



