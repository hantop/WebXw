<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="FundSplitDetailAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="FundSplitDetailAction_findByPage" method="findByPage" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="list">/creditapp/fund/FundSplitDetail_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="FundSplitDetailAction_input" method="input" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="input">/creditapp/fund/FundSplitDetail_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="FundSplitDetailAction_del" method="del" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="list">/creditapp/fund/FundSplitDetail_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="FundSplitDetailAction_getById" method="getById" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="detail">/creditapp/fund/FundSplitDetail_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="FundSplitDetailActionInsert" method="insert" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="input">/creditapp/fund/FundSplitDetail_Insert.jsp</result>
			<result name="detail">/creditapp/fund/FundSplitDetail_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="FundSplitDetailActionUpdate" method="update" class="app.creditapp.fund.action.FundSplitDetailAction">
			<result name="input">/creditapp/fund/FundSplitDetail_Detail.jsp</result>
			<result name="detail">/creditapp/fund/FundSplitDetail_Detail.jsp</result>
		</action>
	</package>
</struts>



