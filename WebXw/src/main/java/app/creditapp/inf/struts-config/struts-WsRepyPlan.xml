<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="WsRepyPlanAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="WsRepyPlanAction_findByPage" method="findByPage" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="list">/creditapp/inf/WsRepyPlan_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="WsRepyPlanAction_input" method="input" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="input">/creditapp/inf/WsRepyPlan_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="WsRepyPlanAction_del" method="del" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="list">/creditapp/inf/WsRepyPlan_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="WsRepyPlanAction_getById" method="getById" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="detail">/creditapp/inf/WsRepyPlan_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="WsRepyPlanActionInsert" method="insert" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="input">/creditapp/inf/WsRepyPlan_Insert.jsp</result>
			<result name="detail">/creditapp/inf/WsRepyPlan_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="WsRepyPlanActionUpdate" method="update" class="app.creditapp.inf.action.WsRepyPlanAction">
			<result name="input">/creditapp/inf/WsRepyPlan_Detail.jsp</result>
			<result name="detail">/creditapp/inf/WsRepyPlan_Detail.jsp</result>
		</action>
	</package>
</struts>



