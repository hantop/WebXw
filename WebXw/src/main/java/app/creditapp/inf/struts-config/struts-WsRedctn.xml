<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="WsRedctnAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="WsRedctnAction_findByPage" method="findByPage" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="list">/creditapp/inf/WsRedctn_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="WsRedctnAction_input" method="input" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="input">/creditapp/inf/WsRedctn_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="WsRedctnAction_del" method="del" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="list">/creditapp/inf/WsRedctn_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="WsRedctnAction_getById" method="getById" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="detail">/creditapp/inf/WsRedctn_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="WsRedctnActionInsert" method="insert" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="input">/creditapp/inf/WsRedctn_Insert.jsp</result>
			<result name="detail">/creditapp/inf/WsRedctn_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="WsRedctnActionUpdate" method="update" class="app.creditapp.inf.action.WsRedctnAction">
			<result name="input">/creditapp/inf/WsRedctn_Detail.jsp</result>
			<result name="detail">/creditapp/inf/WsRedctn_Detail.jsp</result>
		</action>
	</package>
</struts>



