<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="WsRepyMesAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="WsRepyMesAction_findByPage" method="findByPage" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="list">/creditapp/inf/WsRepyMes_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="WsRepyMesAction_input" method="input" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="input">/creditapp/inf/WsRepyMes_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="WsRepyMesAction_del" method="del" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="list">/creditapp/inf/WsRepyMes_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="WsRepyMesAction_getById" method="getById" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="detail">/creditapp/inf/WsRepyMes_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="WsRepyMesActionInsert" method="insert" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="input">/creditapp/inf/WsRepyMes_Insert.jsp</result>
			<result name="detail">/creditapp/inf/WsRepyMes_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="WsRepyMesActionUpdate" method="update" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="input">/creditapp/inf/WsRepyMes_Detail.jsp</result>
			<result name="detail">/creditapp/inf/WsRepyMes_Detail.jsp</result>
		</action>
		
		<!--扣款信息统计-->
		<action name="WsRepyMesAction_getCountMes" method="getCountMes" class="app.creditapp.inf.action.WsRepyMesAction">
			<result name="list">/creditapp/inf/WsRepyMes_Count.jsp</result>
		</action>
	</package>
</struts>



