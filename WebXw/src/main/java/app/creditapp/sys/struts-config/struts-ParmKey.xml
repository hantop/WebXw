<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="Parmkey" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="ParmKeyAction_findByPage" method="findByPage" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="list">/creditapp/sys/ParmKey_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="ParmKeyAction_input" method="input" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="input">/creditapp/sys/ParmKey_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="ParmKeyAction_del" method="del" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="list">/creditapp/sys/ParmKey_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="ParmKeyAction_getById" method="getById" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="detail">/creditapp/sys/ParmKey_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="ParmKeyActionInsert" method="insert" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="input">/creditapp/sys/ParmKey_Insert.jsp</result>
			<result name="detail">/creditapp/sys/ParmKey_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="ParmKeyActionUpdate" method="update" class="app.creditapp.sys.action.ParmKeyAction">
			<result name="input">/creditapp/sys/ParmKey_Detail.jsp</result>
			<result name="detail">/creditapp/sys/ParmKey_Detail.jsp</result>
		</action>
	</package>
</struts>



