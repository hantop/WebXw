<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="ParmRewAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="ParmRewAction_findByPage" method="findByPage" class="app.creditapp.sys.action.ParmRewAction">
			<result name="list">/creditapp/sys/ParmRew_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="ParmRewAction_input" method="input" class="app.creditapp.sys.action.ParmRewAction">
			<result name="input">/creditapp/sys/ParmRew_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="ParmRewAction_del" method="del" class="app.creditapp.sys.action.ParmRewAction">
			<result name="list">/creditapp/sys/ParmRew_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="ParmRewAction_getById" method="getById" class="app.creditapp.sys.action.ParmRewAction">
			<result name="detail">/creditapp/sys/ParmRew_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="ParmRewActionInsert" method="insert" class="app.creditapp.sys.action.ParmRewAction">
			<result name="input">/creditapp/sys/ParmRew_Insert.jsp</result>
			<result name="detail">/creditapp/sys/ParmRew_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="ParmRewActionUpdate" method="update" class="app.creditapp.sys.action.ParmRewAction">
			<result name="input">/creditapp/sys/ParmRew_Detail.jsp</result>
			<result name="detail">/creditapp/sys/ParmRew_Detail.jsp</result>
		</action>
	</package>
</struts>



