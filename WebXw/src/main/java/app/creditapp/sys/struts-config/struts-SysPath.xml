<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="SysPathAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="SysPathAction_findByPage" method="findByPage" class="app.creditapp.sys.action.SysPathAction">
			<result name="list">/creditapp/sys/SysPath_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="SysPathAction_input" method="input" class="app.creditapp.sys.action.SysPathAction">
			<result name="input">/creditapp/sys/SysPath_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="SysPathAction_del" method="del" class="app.creditapp.sys.action.SysPathAction">
			<result name="list">/creditapp/sys/SysPath_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="SysPathAction_getById" method="getById" class="app.creditapp.sys.action.SysPathAction">
			<result name="detail">/creditapp/sys/SysPath_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="SysPathActionInsert" method="insert" class="app.creditapp.sys.action.SysPathAction">
			<result name="input">/creditapp/sys/SysPath_Insert.jsp</result>
			<result name="detail">/creditapp/sys/SysPath_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="SysPathActionUpdate" method="update" class="app.creditapp.sys.action.SysPathAction">
			<result name="input">/creditapp/sys/SysPath_Detail.jsp</result>
			<result name="detail">/creditapp/sys/SysPath_Detail.jsp</result>
		</action>
	</package>
</struts>



