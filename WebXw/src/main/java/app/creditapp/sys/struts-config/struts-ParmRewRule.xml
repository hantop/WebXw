<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="ParmRewRuleAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="ParmRewRuleAction_findByPage" method="findByPage" class="app.creditapp.sys.action.ParmRewRuleAction">
<!--		<result name="list">/creditapp/sys/ParmRewRule_List.jsp</result>   -->
			<result name="list">/creditapp/sys/ParmRewRule_newList.jsp</result>
		</action>
		<!--分页查询 -->
		<action name="ParmRewRuleAction_findByPageSts" method="findByPageSts" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="list">/creditapp/sys/ParmRewRule_newList.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="ParmRewRuleAction_input" method="input" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="input">/creditapp/sys/ParmRewRule_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="ParmRewRuleAction_del" method="del" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="list">/creditapp/sys/ParmRewRule_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="ParmRewRuleAction_getById" method="getById" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="detail">/creditapp/sys/ParmRewRule_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="ParmRewRuleActionInsert" method="insert" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="input">/creditapp/sys/ParmRewRule_Insert.jsp</result>
			<result name="detail">/creditapp/sys/ParmRewRule_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="ParmRewRuleActionUpdate" method="update" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="input">/creditapp/sys/ParmRewRule_Detail.jsp</result>
			<result name="detail">/creditapp/sys/ParmRewRule_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="SecurityAuditAction_updateRuleSts" method="updateRuleSts" class="app.creditapp.sys.action.ParmRewRuleAction">
			<result name="list">/creditapp/sys/ParmRewRule_newList.jsp</result>
		</action>
	</package>
</struts>



