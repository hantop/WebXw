    <struts>
	<package name="SysRequireTableAction" namespace="/" extends="struts-cms">
		<action name="SysRequireTableAction_*" method="{1}" class="app.creditapp.sysConfig.action.SysRequireTableAction">
			<result name="list">/creditapp/sys/SysRequireTable_List.jsp</result>
			<result name="input">/creditapp/sys/SysRequireTable_Insert.jsp</result>
			<result name="detail">/creditapp/sys/SysRequireTable_Detail.jsp</result>
