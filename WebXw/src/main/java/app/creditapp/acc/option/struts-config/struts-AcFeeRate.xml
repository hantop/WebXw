<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcFeeRateAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AcFeeRateAction_*" method="{1}" class="app.creditapp.acc.option.action.AcFeeRateAction">
			<result name="list">/creditapp/acc/option/AcFeeRate_List.jsp</result>
			<result name="listForPop">/creditapp/acc/option/AcFeeRate_ListForPop.jsp</result>
			<result name="input">/creditapp/acc/option/AcFeeRate_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeRate_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcFeeRateActionInsert" method="insert" class="app.creditapp.acc.option.action.AcFeeRateAction">
			<result name="input">/creditapp/acc/option/AcFeeRate_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeRate_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcFeeRateActionUpdate" method="update" class="app.creditapp.acc.option.action.AcFeeRateAction">
			<result name="input">/creditapp/acc/option/AcFeeRate_Detail.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeRate_Detail.jsp</result>
		</action>
	</package>
</struts>



