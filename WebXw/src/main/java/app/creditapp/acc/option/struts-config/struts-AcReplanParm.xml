<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcReplanParmAction" namespace="/" extends="struts-cms">
	
		<action name="AcReplanParmAction_*" method="{1}" class="app.creditapp.acc.option.action.AcReplanParmAction">
			<result name="list">/creditapp/acc/option/AcReplanParm_List.jsp</result>
			<result name="input">/creditapp/acc/option/AcReplanParm_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanParm_Detail.jsp</result>
			<result name="tab">/creditapp/acc/option/AcReplanParm_Tab.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcReplanParmActionInsert" method="insert" class="app.creditapp.acc.option.action.AcReplanParmAction">
			<result name="input">/creditapp/acc/option/AcReplanParm_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanParm_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcReplanParmActionUpdate" method="update" class="app.creditapp.acc.option.action.AcReplanParmAction">
			<result name="input">/creditapp/acc/option/AcReplanParm_Detail.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanParm_Detail.jsp</result>
		</action>
	</package>
</struts>



