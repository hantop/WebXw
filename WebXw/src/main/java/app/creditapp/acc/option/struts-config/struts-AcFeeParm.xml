<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcFeeParmAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AcFeeParmAction_*" method="{1}" class="app.creditapp.acc.option.action.AcFeeParmAction">
			<result name="list">/creditapp/acc/option/AcFeeParm_List.jsp</result>
			<result name="input">/creditapp/acc/option/AcFeeParm_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeParm_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcFeeParmActionInsert" method="insert" class="app.creditapp.acc.option.action.AcFeeParmAction">
			<result name="input">/creditapp/acc/option/AcFeeParm_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeParm_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcFeeParmActionUpdate" method="update" class="app.creditapp.acc.option.action.AcFeeParmAction">
			<result name="input">/creditapp/acc/option/AcFeeParm_Detail.jsp</result>
			<result name="detail">/creditapp/acc/option/AcFeeParm_Detail.jsp</result>
		</action>
	</package>
</struts>



