<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcReplanSecAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AcReplanSecAction_*" method="{1}" class="app.creditapp.acc.option.action.AcReplanSecAction">
			<result name="list">/creditapp/acc/option/AcReplanSec_List.jsp</result>
			<result name="input">/creditapp/acc/option/AcReplanSec_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanSec_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcReplanSecActionInsert" method="insert" class="app.creditapp.acc.option.action.AcReplanSecAction">
			<result name="input">/creditapp/acc/option/AcReplanSec_Insert.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanSec_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcReplanSecActionUpdate" method="update" class="app.creditapp.acc.option.action.AcReplanSecAction">
			<result name="input">/creditapp/acc/option/AcReplanSec_Detail.jsp</result>
			<result name="detail">/creditapp/acc/option/AcReplanSec_Detail.jsp</result>
		</action>
	</package>
</struts>



