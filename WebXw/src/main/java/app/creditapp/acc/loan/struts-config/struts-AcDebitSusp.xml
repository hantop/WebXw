<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcDebitSuspAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AcDebitSuspAction_findByPage" method="findByPage" class="app.creditapp.acc.loan.action.AcDebitSuspAction">
			<result name="list">/creditapp/acc/AcDebitSusp_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="AcDebitSuspAction_input" method="input" class="app.creditapp.acc.loan.action.AcDebitSuspAction">
			<result name="input">/creditapp/acc/AcDebitSusp_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="AcDebitSuspAction_del" method="del" class="app.creditapp.acc.loan.action.AcDebitSuspAction">
			<result name="list">/creditapp/acc/AcDebitSusp_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="AcDebitSuspAction_getById" method="getById" class="app.creditapp.acc.loan.action.AcDebitSuspAction">
			<result name="detail">/creditapp/acc/AcDebitSusp_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcDebitSuspActionInsert" method="insert" class="app.creditapp.acc.action.loan.AcDebitSuspAction">
			<result name="input">/creditapp/acc/AcDebitSusp_Insert.jsp</result>
			<result name="detail">/creditapp/acc/AcDebitSusp_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcDebitSuspActionUpdate" method="update" class="app.creditapp.acc.action.loan.AcDebitSuspAction">
			<result name="input">/creditapp/acc/AcDebitSusp_Detail.jsp</result>
			<result name="detail">/creditapp/acc/AcDebitSusp_Detail.jsp</result>
		</action>
		
		<!--测试 -->
		<action name="AcDebitSuspAction_testValidateAcDebitSusp" method="testValidateAcDebitSusp" class="app.creditapp.acc.loan.action.AcDebitSuspAction">
			<result name="detail">/creditapp/acc/AcDebitSusp_Detail.jsp</result>
		</action>
	</package>
</struts>



