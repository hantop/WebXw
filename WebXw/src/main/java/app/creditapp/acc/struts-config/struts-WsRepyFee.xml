<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="WsRepyFeeAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="WsRepyFeeAction_findByPage" method="findByPage" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="list">/creditapp/acc/WsRepyFee_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="WsRepyFeeAction_input" method="input" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="input">/creditapp/acc/WsRepyFee_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="WsRepyFeeAction_del" method="del" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="list">/creditapp/acc/WsRepyFee_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="WsRepyFeeAction_getById" method="getById" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="detail">/creditapp/acc/WsRepyFee_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="WsRepyFeeActionInsert" method="insert" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="input">/creditapp/acc/WsRepyFee_Insert.jsp</result>
			<result name="detail">/creditapp/acc/WsRepyFee_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="WsRepyFeeActionUpdate" method="update" class="app.creditapp.acc.action.WsRepyFeeAction">
			<result name="input">/creditapp/acc/WsRepyFee_Detail.jsp</result>
			<result name="detail">/creditapp/acc/WsRepyFee_Detail.jsp</result>
		</action>
	</package>
</struts>



