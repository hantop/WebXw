<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AcLnIntstLogAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AcLnIntstLogAction_*" method="{1}" class="app.creditapp.acc.log.action.AcLnIntstLogAction">
			<result name="list">/creditapp/acc/log/AcLnIntstLog_List.jsp</result>
			<result name="input">/creditapp/acc/log/AcLnIntstLog_Insert.jsp</result>
			<result name="detail">/creditapp/acc/log/AcLnIntstLog_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AcLnIntstLogActionInsert" method="insert" class="app.creditapp.acc.log.action.AcLnIntstLogAction">
			<result name="input">/creditapp/acc/log/AcLnIntstLog_Insert.jsp</result>
			<result name="detail">/creditapp/acc/log/AcLnIntstLog_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AcLnIntstLogActionUpdate" method="update" class="app.creditapp.acc.log.action.AcLnIntstLogAction">
			<result name="input">/creditapp/acc/log/AcLnIntstLog_Detail.jsp</result>
			<result name="detail">/creditapp/acc/log/AcLnIntstLog_Detail.jsp</result>
		</action>
	</package>
</struts>



