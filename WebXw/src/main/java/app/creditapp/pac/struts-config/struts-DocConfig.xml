<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="DocConfigAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="DocConfigAction_findByPage" method="findByPage" class="app.creditapp.pac.action.DocConfigAction">
			<result name="list">/creditapp/pac/DocConfig_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="DocConfigAction_input" method="input" class="app.creditapp.pac.action.DocConfigAction">
			<result name="input">/creditapp/pac/DocConfig_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="DocConfigAction_del" method="del" class="app.creditapp.pac.action.DocConfigAction">
			<result name="list">/creditapp/pac/DocConfig_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="DocConfigAction_getById" method="getById" class="app.creditapp.pac.action.DocConfigAction">
			<result name="detail">/creditapp/pac/DocConfig_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="DocConfigActionInsert" method="insert" class="app.creditapp.pac.action.DocConfigAction">
			<result name="input">/creditapp/pac/DocConfig_Insert.jsp</result>
			<result name="detail">/creditapp/pac/DocConfig_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="DocConfigActionUpdate" method="update" class="app.creditapp.pac.action.DocConfigAction">
			<result name="input">/creditapp/pac/DocConfig_Detail.jsp</result>
			<result name="detail">/creditapp/pac/DocConfig_Detail.jsp</result>
		</action>
	</package>
</struts>



