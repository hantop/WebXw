<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AftAssetPoolAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AftAssetPoolAction_findByPage" method="findByPage" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="list">/creditapp/aft/AftAssetPool_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="AftAssetPoolAction_input" method="input" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="input">/creditapp/aft/AftAssetPool_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="AftAssetPoolAction_del" method="del" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="list">/creditapp/aft/AftAssetPool_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="AftAssetPoolAction_getById" method="getById" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="detail">/creditapp/aft/AftAssetPool_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AftAssetPoolActionInsert" method="insert" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="input">/creditapp/aft/AftAssetPool_Insert.jsp</result>
			<result name="detail">/creditapp/aft/AftAssetPool_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AftAssetPoolActionUpdate" method="update" class="app.creditapp.aft.action.AftAssetPoolAction">
			<result name="input">/creditapp/aft/AftAssetPool_Detail.jsp</result>
			<result name="detail">/creditapp/aft/AftAssetPool_Detail.jsp</result>
		</action>
	</package>
</struts>



