<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AftAssetRelAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AftAssetRelAction_findByPage" method="findByPage" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="list">/creditapp/aft/AftAssetRel_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="AftAssetRelAction_input" method="input" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="input">/creditapp/aft/AftAssetRel_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="AftAssetRelAction_del" method="del" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="list">/creditapp/aft/AftAssetRel_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="AftAssetRelAction_getById" method="getById" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="detail">/creditapp/aft/AftAssetRel_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AftAssetRelActionInsert" method="insert" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="input">/creditapp/aft/AftAssetRel_Insert.jsp</result>
			<result name="detail">/creditapp/aft/AftAssetRel_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AftAssetRelActionUpdate" method="update" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="input">/creditapp/aft/AftAssetRel_Detail.jsp</result>
			<result name="detail">/creditapp/aft/AftAssetRel_Detail.jsp</result>
		</action>
		<action name="AftAssetRelActionUpdateForAmt" method="updateForAmt" class="app.creditapp.aft.action.AftAssetRelAction">
			<result name="input">/creditapp/aft/AftAssetRel_Detail.jsp</result>
			<result name="detail">/creditapp/aft/AftAssetRel_Detail.jsp</result>
		</action>
	</package>
</struts>



