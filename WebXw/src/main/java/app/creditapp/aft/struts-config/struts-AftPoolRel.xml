<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AftPoolRelAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AftPoolRelAction_findByPage" method="findByPage" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="list">/creditapp/aft/AftPoolRel_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="AftPoolRelAction_input" method="input" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="input">/creditapp/aft/AftPoolRel_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="AftPoolRelAction_del" method="del" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="list">/creditapp/aft/AftPoolRel_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="AftPoolRelAction_getById" method="getById" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="detail">/creditapp/aft/AftPoolRel_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AftPoolRelActionInsert" method="insert" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="input">/creditapp/aft/AftPoolRel_Insert.jsp</result>
			<result name="detail">/creditapp/aft/AftPoolRel_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AftPoolRelActionUpdate" method="update" class="app.creditapp.aft.action.AftPoolRelAction">
			<result name="input">/creditapp/aft/AftPoolRel_Detail.jsp</result>
			<result name="detail">/creditapp/aft/AftPoolRel_Detail.jsp</result>
		</action>
	</package>
</struts>



