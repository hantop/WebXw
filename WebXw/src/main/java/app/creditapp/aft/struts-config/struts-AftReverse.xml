<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="AftReverseAction" namespace="/" extends="struts-cms">
	
	    <!--分页查询 -->
		<action name="AftReverseAction_*" method="{1}" class="app.creditapp.aft.action.AftReverseAction">
			<result name="list">/creditapp/aft/AftReverse_List.jsp</result>
		</action>
		
		<!--获取新增页面-->
		<action name="AftReverseAction_input" method="input" class="app.creditapp.aft.action.AftReverseAction">
			<result name="input">/creditapp/aft/AftReverse_Insert.jsp</result>
		</action>
		
		<!--删除 -->
		<action name="AftReverseAction_del" method="del" class="app.creditapp.aft.action.AftReverseAction">
			<result name="list">/creditapp/aft/AftReverse_List.jsp</result>
		</action>
		
		<!--查询 -->
		<action name="AftReverseAction_getById" method="getById" class="app.creditapp.aft.action.AftReverseAction">
			<result name="detail">/creditapp/aft/AftReverse_Detail.jsp</result>
		</action>
		
		<!--新增保存-->
		<action name="AftReverseActionInsert" method="insert" class="app.creditapp.aft.action.AftReverseAction">
			<result name="input">/creditapp/aft/AftReverse_Insert.jsp</result>
			<result name="detail">/creditapp/aft/AftReverse_Detail.jsp</result>
		</action>
		<!--修改保存-->
		<action name="AftReverseActionUpdate" method="update" class="app.creditapp.aft.action.AftReverseAction">
			<result name="input">/creditapp/aft/AftReverse_Detail.jsp</result>
			<result name="detail">/creditapp/aft/AftReverse_Detail.jsp</result>
		</action>
	</package>
</struts>



