		<columnList>
			<!-- 合作机构号 -->
			<column columenName="brNo">
				<canNull>false</canNull>
				<maxLength>10</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
