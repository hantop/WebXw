				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			
			<!-- 合同号 -->
			<column columenName="pactNo">
				<canNull>false</canNull>
				<maxLength>40</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
