				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			<!-- 页码 -->
			<column columenName="pageSize">
				<canNull>true</canNull>
				<maxLength>4</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
