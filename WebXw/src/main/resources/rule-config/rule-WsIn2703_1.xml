<?xml version="1.0" encoding="UTF-8"?>
<ruleRoot>
	<!-- ===========================分割线=========================== -->
	<!-- ====================该验证的表LN_ACCT_MID================== -->
	<!-- =========================================================== -->
	<rules>
		<ruleId>wsIn2703_1Id</ruleId>
		<ruleName>wsIn2703_1Rule</ruleName>
		<columnList>
		    <!--合同号-->
			<column columenName="pactNo">
				<canNull>false</canNull>
				<maxLength>40</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			<!--展期金额-->
			<column columenName="expAmt">
				<canNull>false</canNull>
				<maxLength></maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber>16,2</complexNumber>
			</column>
			<!--展期利率-->
			<column columenName="expRate">
				<canNull>false</canNull>
				<maxLength></maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber>10,6</complexNumber>
			</column>
			<!--展期起始日-->
			<column columenName="expBegDate">
				<canNull>false</canNull>
				<maxLength>8</maxLength>
				<minLength>8</minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			<!--展期到期日-->
			<column columenName="expEndDate">
				<canNull>false</canNull>
				<maxLength>8</maxLength>
				<minLength>8</minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			<!--展期原因-->
			<column columenName="expReason">
				<canNull>true</canNull>
				<maxLength>60</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
		</columnList>
	</rules>
</ruleRoot>