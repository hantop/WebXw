<?xml version="1.0" encoding="UTF-8"?>
<ruleRoot>
	<!-- ===========================分割线=========================== -->
	<!-- ====================该验证的表LN_ACCT_MID================== -->
	<!-- =========================================================== -->
	<rules>
		<ruleId>wsPreAcctId</ruleId>
		<ruleName>lnpreacctRule</ruleName>
		<columnList>
			<!-- 申请ID -->
			<column columenName="appId">
				<canNull>true</canNull>
				<maxLength>14</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 账户用途[1扣款账户2放款账户] -->
			<column columenName="acUse">
				<canNull>true</canNull>
				<maxLength>2</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 放款金额 -->
			<column columenName="acAmt">
				<canNull>true</canNull>
				<maxLength></maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 账户类型[11个人账户12企业账户] -->
			<column columenName="acType">
				<canNull></canNull>
				<maxLength>2</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 账户号码 -->
			<column columenName="acNo">
				<canNull>true</canNull>
				<maxLength>20</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 账户户名 -->
			<column columenName="acName">
				<canNull></canNull>
				<maxLength>60</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 开户银行代码 -->
			<column columenName="bankCode">
				<canNull></canNull>
				<maxLength>3</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 开户银行所在省 -->
			<column columenName="bankProv">
				<canNull></canNull>
				<maxLength>60</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 开户银行所在市 -->
			<column columenName="bankCity">
				<canNull></canNull>
				<maxLength>60</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 开户银行网点 -->
			<column columenName="bankSite">
				<canNull></canNull>
				<maxLength>80</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
		</columnList>
	</rules>
</ruleRoot>