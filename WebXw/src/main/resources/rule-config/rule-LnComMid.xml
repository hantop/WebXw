	<rules>
		<ruleId>wsLnComMidId</ruleId>
		<ruleName>lncommidRule</ruleName>
		<columnList>
			<!-- 申请ID -->
			<column columenName="appId">
				<canNull></canNull>
				<maxLength>14</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 名称 -->
			<column columenName="comName">
				<canNull></canNull>
				<maxLength>30</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 证件类型 -->
			<column columenName="comIdtype">
				<canNull></canNull>
				<maxLength>1</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 证件号 -->
			<column columenName="comIdno">
				<canNull></canNull>
				<maxLength>30</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 联系电话 -->
			<column columenName="comTel">
				<canNull></canNull>
				<maxLength>25</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
		</columnList>
	</rules>
