<?xml version="1.0" encoding="UTF-8"?>
<ruleRoot>
	<!-- ===========================分割线=========================== -->
	<!-- ====================校验正式进件申请信息================== -->
	<!-- =========================================================== -->
	<rules>
		<ruleId>wsIn2301Id</ruleId>
		<ruleName>wsIn2301Rule</ruleName>
		<columnList>
			<!-- 合同号不能为空 -->
			<column columenName="brNo">
				<canNull>false</canNull>
				<maxLength>10</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
			<column columenName="dataCnt">
				<canNull>false</canNull>
				<maxLength>4</maxLength>
				<minLength></minLength>
				<maxValue>2000</maxValue>
				<minValue>1</minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
			</column>
			<column columenName="batNo">
				<canNull>false</canNull>
				<maxLength>30</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
			</column>
		</columnList>
	</rules>
</ruleRoot>