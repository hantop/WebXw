<?xml version="1.0" encoding="UTF-8"?>
<ruleRoot>
	<!-- ===========================分割线=========================== -->
	<!-- ============================================================ -->
	<!-- =========================================================== -->
	<rules>
		<ruleId>fundDetail</ruleId>
		<ruleName>fundDetailRule</ruleName>
		<columnList>
			<!-- 交易类型 -->
			<column columenName="tradeType">
				<canNull>false</canNull>
				<maxLength>2</maxLength>
				<minLength>2</minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
			</column>
			<!-- 发生金额 -->
			<column columenName="txAmt">
				<canNull>false</canNull>
				<maxLength></maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber>16,2</complexNumber>
			</column>
			<!-- 交易日期 -->
			<column columenName="txDate">
				<canNull>false</canNull>
				<maxLength>8</maxLength>
				<minLength>8</minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber>true</onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
			<!-- 备注 -->
			<column columenName="filler">
				<canNull>true</canNull>
				<maxLength>200</maxLength>
				<minLength></minLength>
				<maxValue></maxValue>
				<minValue></minValue>
				<startStr></startStr>
				<endStr></endStr>
				<containStr></containStr>
				<onlyNumber></onlyNumber>
				<regexpType></regexpType>
				<complexNumber></complexNumber>
			</column>
		</columnList>
	</rules>
</ruleRoot>